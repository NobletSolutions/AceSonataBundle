  "type": "symfony-bundle",
  "description": "This bundle integrates the sonata admin panel and the ace bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Nathanael David Noblet",
      "email": "nathanael@noblet.ca"
    },
    {
      "name": "Mark Woynarski",
      "email": "mark.woynarski@nobletdesign.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/symfony": "^2.8|^3.0",
    "ns/ace-bundle": "^1.5|^2.0|^3.0",
    "sonata-project/admin-bundle": "^3.0"
  },
  "require-dev": {
    "liip/rmt": "~1.1"
  },
  "autoload": {
    "psr-4": { "NS\\AceSonataBundle\\": "src" }
